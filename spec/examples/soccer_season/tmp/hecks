hecks generate:domain -n soccer_season
hecks generate:domain_object -t aggregate -n Pitches --head_name Pitch -a name:Value
hecks generate:domain_object -t aggregate -n Teams --head_name Team -a name:Value
hecks generate:domain_object -t aggregate -n Players --head_name Player -a name:Value team:Teams::Team
hecks generate:domain_object -t aggregate -n Matches --head_name Match -a fixture:Fixture result:Result pitch:Pitches::Pitch goals:[Goal] teams:Teams::[Team]
hecks generate:domain_object -t value_object -n Result -m Matches -a winner:Teams::Team type:Value
hecks generate:domain_object -t value_object -n Goal -m Matches -a time:Value player:Players::Player
hecks generate:domain_object -t value_object -n Fixture -m Matches -a season:Value date:Value time:Value
hecks generate:domain_object -t reference -n TeamReference -m Players -r Teams::Team
hecks generate:domain_object -t reference -n PitchReference -m Matches -r Pitches::Pitch
hecks generate:domain_object -t reference -n TeamReference -m Matches -r Teams::Team

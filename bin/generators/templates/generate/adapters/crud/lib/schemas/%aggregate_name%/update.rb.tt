module <%= camelized_domain_name %>
  module Schemas
    module <%= aggregate_module_name %>
      Update = Dry::Validation.Schema do
        required(:id).filled(:int?)
        required(:attributes).schema do
        <%- attributes.each do |name, value| -%>
          optional(:<%= name %>).filled(:str?)
        <%- end -%>
        end
      end
    end
  end
end
